{
  "name": "vv-memberwebapp",
  "version": "1.0.0",
  "description": "Video visits application for Members",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "test": "jest --runInBand --watch",
    "coverage": "jest --coverage"
  },
  "author": "TPMG",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|ogg|oga)$": "<rootDir>/dummy.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/dummy.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-observable": "^1.1.2",
    "bootstrap": "^4.1.3",
    "crypto-js": "^3.1.9-1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "npm": "^6.5.0",
    "popper.js": "^1.15.0",
    "raf": "^3.4.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.11.0",
    "redux": "^4.0.1",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^2.0.0",
    "file-loader": "^2.0.0",
    "gifsicle": "4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
