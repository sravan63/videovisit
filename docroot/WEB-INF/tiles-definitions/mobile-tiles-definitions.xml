<tiles-definitions>

	<!-- Application Template -->
	<definition name=".mobileWebPageTemplate" template="/WEB-INF/tiles-templates/mobileLayout.jsp">
		<put-attribute name="title" value="Video Visits Mobile" type="string" />
		<put-attribute name="header" value="/WEB-INF/jsp/mobile/common/header.jsp" />
